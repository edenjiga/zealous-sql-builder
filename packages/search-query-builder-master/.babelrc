{
    "ignore": [
        "node_modules/**"
    ],
    "env": {
        "development": {
            "presets": [
                "react",
                "stage-0"
            ]
        },
        "production": {
            "presets": [
                "flow",
                [
                    "react",
                    {
                        "modules": false
                    }
                ],
                "stage-0"
            ],
            "plugins": [
                "external-helpers"
            ]
        }
    }
}